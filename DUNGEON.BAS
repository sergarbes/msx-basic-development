100 SCREEN 2,0,0
110 GOSUB 2000

200 X=9: Y=9
210 ' WALLS
210 LINE (8,8)-(128,128),15,B
220 ' INTERNAL WALLS
220 LINE (8,32)-(80,40),15,BF
230 LINE -(72,64),15,BF
240 LINE (128,80)-STEP(-80,8),15,BF
250 LINE -STEP(8,-36),15,BF

300 S=STICK(0)
310 IF S=1 AND POINT(X+3,Y-1)<>15 THEN Y=Y-1
330 IF S=3 AND POINT(X+7,Y+3)<>15 THEN X=X+1
350 IF S=5 AND POINT(X+3,Y+7)<>15 THEN Y=Y+1
370 IF S=7 AND POINT(X-1,Y+3)<>15 THEN X=X-1

700 ' Y OF PUT SPRITE SHIFT BUG CORRECTION
700 PUT SPRITE 0,(X,Y-1),8,0

800 GOTO 300

1000 DATA 38,7C,FE,FE,FE,7C,38
1010 DATA *

2000 S=BASE(9)
2010 READ R$
2020 IF R$="*" THEN RETURN ELSE VPOKE S,VAL("&H"+R$):S=S+1
2030 GOTO 2010
